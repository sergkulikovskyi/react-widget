{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "no-multi-spaces": ["error", { "exceptions": { "ImportDeclaration": true } }],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": ["off", { "ignorePureComponents": false }]
  }
}
